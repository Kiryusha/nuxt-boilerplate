{
  "name": "nuxt-boilerplate",
  "version": "1.0.0",
  "description": "RATIO Project",
  "author": "Dmitriy Kulikov",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:host": "cross-env NUXT_HOST=0.0.0.0 nuxt",
    "dev:all": "concurrently --kill-others \"yarn dev\" \"yarn docs:dev\"",
    "build": "nuxt build",
    "build:test": "nuxt build -a",
    "start": "nuxt start",
    "start:host": "cross-env NUXT_HOST=0.0.0.0 nuxt start",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "deploy": "./node_modules/.bin/env-cmd ./scripts/deploy.sh"
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "nuxt": "^2.0.0",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/component-cache": "^1.1.5",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/svg-sprite": "^0.4.10",
    "svg4everybody": "^2.1.9",
    "lazysizes": "^5.2.0",
    "nuxt-user-agent": "^1.2.2",
    "vue-mq": "^1.0.1",
    "swiper": "^5.3.6",
    "element-ui": "^2.4.11"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "env-cmd": "^10.1.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "husky": "^4.0.0",
    "image-size": "^0.8.3",
    "jest": "^24.1.0",
    "lint-staged": "^10.0.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "sharp": "^0.25.2",
    "stylelint": "^10.1.0",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.14.2",
    "vue-jest": "^4.0.0-0",
    "vuepress": "^1.3.1",
    "webp-converter": "^2.2.3"
  }
}
